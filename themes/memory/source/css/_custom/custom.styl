// Custom styles.
// 滚动条样式
::-webkit-scrollbar {
  width: 6px;
} /* 这是针对缺省样式 (必须的) */

::-webkit-scrollbar-track {
      background-color: #fff;
} /* 滚动条的滑轨背景颜色 */

::-webkit-scrollbar-thumb {
  background-color: #ACAAAA;
  border-radius: 2.5px;
} /* 滑块颜色 */

.custom-post-detail {
  position: absolute;
  bottom: 50px;
  margin: 0 auto;
}
